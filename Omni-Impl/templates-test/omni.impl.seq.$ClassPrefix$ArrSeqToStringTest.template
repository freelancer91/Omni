  OmniCollection.Of$ClassPrefix$ seq=root.subList(0,0);
#ELSE
  OmniCollection.Of$ClassPrefix$ seq=new STRUCTNAME(length);
#ENDIF
  ArrayList arrayList=new ArrayList(length);
