private static final $Comparator$ Unstable=(val1,val2)->
{
  //even comes first
  if((val1&0b1)==0)
  {
    //val1 is even
    if((val2&0b1)!=0)
    {
      return -1;
    }
  }
  else if((val2&0b1)==0)
  {
    return 1; 
  }
  return 0;
};
