  arr[begin]=arr[less-1];
  arr[less-1]=pivot1;
  arr[end]=arr[great+1];
  arr[great+1]=pivot2;
#IFSWITCH LEFTMOST==true
  quickMETHODNAMEleftmost(arr,begin,less-2);
#ELSE
