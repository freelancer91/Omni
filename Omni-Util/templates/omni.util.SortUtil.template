#ENDIF
  quickMETHODNAME(arr,great+2,end);
  if(less<e1 && e5<great)
  {
    #MACRO WhileClause(Equal,arr[less],pivot1)
    {
      ++less;
    }
    #MACRO WhileClause(Equal,arr[great],pivot2)
    {
      --great;
    }
    outer: for(int k=less;k<=great;++k)
    {
      ARRTYPE ak;
      #MACRO IfClause(Equal,ak=arr[k],pivot1)
      {
        arr[k]=arr[less];
